<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="mysqlImpl" class="com.demo01.dao.UserDaoMysqlImpl"/>
    <bean id="oracleImpl" class="com.demo01.dao.UserOracleImpl"/>
    <bean id="Dao" class="com.demo01.dao.UserDaoImpl"/>

    <bean id="UserServiceImpl" class="com.demo01.service.UserServiceImpl">
      <property name="userDao" ref="oracleImpl"/>
    </bean>


</beans>
